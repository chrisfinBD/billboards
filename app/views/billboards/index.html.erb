<% @billboards.each do |billboard| %>
  <p> 
  <%= billboard.body %>
  &nbsp;&mdash;&nbsp;
  <%= link_to("edit", edit_billboard_path(billboard.id)) %>
  -
  <%= link_to("delete", billboard_path(billboard.id), method: :delete, data: {confirm: "Are you sure you want to delete #{billboard.id}?"}) %>
  </p>
<% end %>
